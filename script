reanimate:NewMode("2",function()
    local Root = workspace.Camera.CameraSubject.RootPart
    if Root.Velocity.y > 1 then -- jump
    --jump lerp here
    elseif Root.Velocity.y < -1 then -- fall
    --fall lerp here
    elseif Root.Velocity.Magnitude < 2> 20 then -- run
    --run lerp here
    end
    end)

reanimate:NewMode("3",function()
    local Root = workspace.Camera.CameraSubject.RootPart
    if Root.Velocity.y > 1 then -- jump
    --jump lerp here
    elseif Root.Velocity.y < -1 then -- fall
    --fall lerp here
    elseif Root.Velocity.Magnitude < 2> 20 then -- run
    --run lerp here
    end
    end)


reanimate:Clickbind("Down",1,function()
    reanimate:SetAnimation("attack1",1)-- you can change the time here (in seconds)
    reanimate:SetMode("1")-- set mode number eg 1 2 3
end)


